{
  "orders": {
    "schema": {
      "order_id": "string",
      "customer_id": "string",
      "order_date": "timestamp",
      "status": "string",
      "amount": "double"
    },
    "quality_rules": {
      "not_null": ["order_id", "customer_id"],
      "positive_values": ["amount"],
      "accepted_values": {
        "status": ["pending", "shipped", "delivered", "cancelled"]
      }
    },
    "freshness_hours": 24,
    "owner": "data-eng-team"
  }
}
